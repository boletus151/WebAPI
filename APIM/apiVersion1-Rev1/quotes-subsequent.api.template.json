{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "apimServiceName": {
      "type": "string"
    },
    "quotes-ServiceUrl": {
      "type": "string"
    }
  },
  "resources": [
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/apis",
      "name": "[concat(parameters('apimServiceName'), '/quotes')]",
      "dependsOn": [],
      "properties": {
        "description": "Basic operations with some authentication",
        "type": "https",
        "apiRevision": "1",
        "apiVersion": "v1",
        "isCurrent": true,
        "apiVersionSetId": "[resourceId('Microsoft.ApiManagement/service/apiVersionSets', parameters('apimServiceName'), 'v1')]",
        "subscriptionRequired": false,
        "displayName": "Quotes Services",
        "serviceUrl": "[parameters('quotes-ServiceUrl')]",
        "path": "quotes",
        "protocols": [
          "https"
        ],
        "apiType": "https"
      }
    }
  ]
}